{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport { mapMutations } from \"vuex\";\nexport default {\n  name: 'Login',\n  data() {\n    return {\n      loginObj: {\n        email: \"\",\n        pass: \"\"\n      },\n      matchUser: undefined,\n      errors: []\n    };\n  },\n  methods: {\n    ...mapMutations([\"setUser\"]),\n    scrollToTop() {\n      window.scrollTo(0, 0);\n    },\n    async getMatchUser(email) {\n      let data = await axios.get('/users/' + email);\n      this.matchUser = data.data;\n    },\n    async handleSubmit(e) {\n      this.errors = [];\n      if (!this.loginObj.email) {\n        this.errors.push(\"Entering a email is required\");\n      } else {\n        if (!/[a-z0-9._%+-]+@[a-z0-9.-]+\\.[a-z]{2,3}$/.test(this.loginObj.email)) {\n          this.errors.push('Email must be valid');\n        }\n      }\n      if (!this.loginObj.pass) {\n        this.errors.push('Password is required');\n      }\n      if (!this.errors.length == 0) {\n        e.preventDefault();\n      } else {\n        e.preventDefault();\n        await this.getMatchUser(this.loginObj.email);\n        if (!this.matchUser) {\n          this.errors.push(\"Incorrect email or password!\");\n        } else {\n          if (this.matchUser.user_password === this.loginObj.pass) {\n            this.matchUser.user_password = \"\";\n            this.setUser(this.matchUser);\n            this.$router.push(\"/\");\n          } else {\n            this.errors.push(\"Incorrect email or password!\");\n          }\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","mapMutations","name","data","loginObj","email","pass","matchUser","undefined","errors","methods","scrollToTop","window","scrollTo","getMatchUser","get","handleSubmit","e","push","test","length","preventDefault","user_password","setUser","$router"],"sources":["C:\\Users\\visha\\Downloads\\vedeshi khana\\restaurant-ordering-system-master\\frontend\\src\\pages\\Login.vue"],"sourcesContent":["<template>\n    <div class=\"login-container\">\n        <div class=\"login-form-container\">\n            <form id=\"loginForm\" @submit=\"handleSubmit\" novalidate autocomplete=\"off\">\n                <h3>LOGIN</h3>\n\n                <div v-if=\"errors.length\" class=\"error-box\">\n                    <ul>\n                        <li v-for=\"error in errors\" :key=\"error\">{{ error }}</li>\n                    </ul>\n                </div>\n\n                <div class=\"form-group\">\n                    <input type=\"email\" id=\"uEmail\" name=\"uEmail\" class=\"form-control\" placeholder=\"enter your email\"\n                        v-model=\"loginObj.email\" />\n                </div>\n\n                <div class=\"form-group\">\n                    <input type=\"password\" id=\"uPass\" name=\"uPass\" class=\"form-control\"\n                        placeholder=\"enter your password\" v-model=\"loginObj.pass\" />\n                </div>\n\n                <div class=\"form-group\">\n                    <input type=\"submit\" value=\"login now\" class=\"btn\">\n                    <p>don't have an account? <router-link @click=\"scrollToTop()\" to=\"/register\">create one\n                        </router-link>\n                    </p>\n                </div>\n            </form>\n        </div>\n    </div>\n</template>\n\n\n<script>\nimport axios from \"axios\";\nimport { mapMutations } from \"vuex\";\nexport default {\n    name: 'Login',\n\n    data() {\n        return {\n            loginObj: { email: \"\", pass: \"\" },\n            matchUser: undefined,\n            errors: [],\n        }\n    },\n\n    methods: {\n        ...mapMutations([\"setUser\"]),\n\n        scrollToTop() {\n            window.scrollTo(0, 0);\n        },\n\n        async getMatchUser(email) {\n            let data = await axios.get('/users/' + email);\n            this.matchUser = data.data;\n        },\n\n        async handleSubmit(e) {\n            this.errors = [];\n\n            if (!this.loginObj.email) {\n                this.errors.push(\"Entering a email is required\");\n            }\n            else {\n                if (!/[a-z0-9._%+-]+@[a-z0-9.-]+\\.[a-z]{2,3}$/.test(this.loginObj.email)) {\n                    this.errors.push('Email must be valid');\n                }\n            }\n\n\n            if (!this.loginObj.pass) {\n                this.errors.push('Password is required');\n            }\n\n            if (!this.errors.length == 0) {\n                e.preventDefault();\n            }\n            else {\n                e.preventDefault();\n                await this.getMatchUser(this.loginObj.email);\n                if (!this.matchUser) {\n                    this.errors.push(\"Incorrect email or password!\")\n                }\n                else {\n                    if (this.matchUser.user_password === this.loginObj.pass) {\n                        this.matchUser.user_password = \"\";\n                        this.setUser(this.matchUser);\n                        this.$router.push(\"/\");\n                    }\n                    else {\n                        this.errors.push(\"Incorrect email or password!\")\n                    }\n                }\n            }\n        }\n\n    }\n\n}\n</script>\n\n<style scoped>\n.login-container {\n    padding: 2rem 9%;\n}\n\n.login-container .login-form-container {\n    background-color: #fff;\n    height: 90vh;\n}\n\n.login-container .login-form-container form {\n    position: absolute;\n    top: 50%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n    max-width: 40rem;\n    width: 100%;\n    box-shadow: 0 1rem 1rem rgba(0, 0, 0, 0.05);\n    border: 0.1rem solid rgba(0, 0, 0, 0.2);\n    padding: 2rem;\n    border-radius: .5rem;\n    animation: fadeUp .4s linear;\n}\n\n.login-container .login-form-container form h3 {\n    padding-bottom: 1rem;\n    font-size: 2rem;\n    font-weight: bolder;\n    text-transform: uppercase;\n    color: #130f40;\n    margin: 0;\n}\n\n.login-container .login-form-container form .form-control {\n    margin: .7rem 0;\n    border-radius: .5rem;\n    background: #f7f7f7;\n    padding: 2rem 1.2rem;\n    font-size: 1.6rem;\n    color: #130f40;\n    text-transform: none;\n    width: 100%;\n    border: none;\n}\n\n.login-container .login-form-container form .btn {\n    margin-bottom: 1rem;\n    margin-top: 1rem;\n    width: 100%;\n}\n\n.login-container .login-form-container form p {\n    padding-top: 1rem;\n    font-size: 1.5rem;\n    color: #666;\n    margin: 0;\n}\n\n.login-container .login-form-container form p a {\n    color: #27ae60;\n}\n\n.login-container .login-form-container form p a:hover {\n    color: #130f40;\n    text-decoration: underline;\n}\n\n.login-container .login-form-container form .error-box {\n    background-color: #fff9fa;\n    box-sizing: border-box;\n    border: 2px solid rgba(255, 66, 79, .2);\n    border-radius: 2px;\n    font-size: 12px;\n    margin-bottom: 20px;\n}\n\n.login-container .login-form-container form .error-box ul {\n    list-style-type: none;\n    margin: 0;\n    padding: 10px 0px;\n}\n\n.login-container .login-form-container form .error-box ul li {\n    padding-left: 10px;\n    color: rgb(182, 0, 0);\n}\n</style>"],"mappings":";AAmCA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,YAAW,QAAS,MAAM;AACnC,eAAe;EACXC,IAAI,EAAE,OAAO;EAEbC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,QAAQ,EAAE;QAAEC,KAAK,EAAE,EAAE;QAAEC,IAAI,EAAE;MAAG,CAAC;MACjCC,SAAS,EAAEC,SAAS;MACpBC,MAAM,EAAE;IACZ;EACJ,CAAC;EAEDC,OAAO,EAAE;IACL,GAAGT,YAAY,CAAC,CAAC,SAAS,CAAC,CAAC;IAE5BU,WAAWA,CAAA,EAAG;MACVC,MAAM,CAACC,QAAQ,CAAC,CAAC,EAAE,CAAC,CAAC;IACzB,CAAC;IAED,MAAMC,YAAYA,CAACT,KAAK,EAAE;MACtB,IAAIF,IAAG,GAAI,MAAMH,KAAK,CAACe,GAAG,CAAC,SAAQ,GAAIV,KAAK,CAAC;MAC7C,IAAI,CAACE,SAAQ,GAAIJ,IAAI,CAACA,IAAI;IAC9B,CAAC;IAED,MAAMa,YAAYA,CAACC,CAAC,EAAE;MAClB,IAAI,CAACR,MAAK,GAAI,EAAE;MAEhB,IAAI,CAAC,IAAI,CAACL,QAAQ,CAACC,KAAK,EAAE;QACtB,IAAI,CAACI,MAAM,CAACS,IAAI,CAAC,8BAA8B,CAAC;MACp